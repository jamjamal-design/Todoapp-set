{
  "rewrites": [
    {
      "source": "/",
      "destination": "/signup.html"
    },
    {
      "source": "/signup",
      "destination": "/signup.html"
    },
    {
      "source": "/signin",
      "destination": "/signin.html"
    },
    {
      "source": "/login",
      "destination": "/signin.html"
    },
    {
      "source": "/dashboard",
      "destination": "/dashboard.html"
    },
    {
      "source": "/app",
      "destination": "/dashboard.html"
    },
    {
      "source": "/home",
      "destination": "/dashboard.html"
    }
  ],
  "redirects": [
    {
      "source": "/index.html",
      "destination": "/signup.html",
      "permanent": true
    },
    {
      "source": "/index",
      "destination": "/signup.html",
      "permanent": true
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "Referrer-Policy",
          "value": "strict-origin-when-cross-origin"
        }
      ]
    },
    {
      "source": "/(.*)\\.(css|js)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/(.*)\\.(html)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=0, must-revalidate"
        }
      ]
    }
  ],
  "cleanUrls": true,
  "trailingSlash": false
}